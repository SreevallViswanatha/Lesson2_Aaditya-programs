#include <stdio.h>
#include <stdlib.h>
#include<math.h>
//Find the absolute value of a number entered through the keyboard.
int main()
{

    int num;
    printf("\nEnter a number: ");
    scanf("%d", &num);
    if(num<0)
    {
        num=num*-1;
    }

    printf("The absolute value is: %d", num);
}
