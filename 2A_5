#include <stdio.h>
#include <stdlib.h>
//Take two numbers from the user and display the largest.
int main()
{

    int firstnumber, secondnumber;
    printf("\nEnter the first number: ");
    scanf("%d", &firstnumber);
    printf("\nEnter the second number: ");
    scanf("%d", &secondnumber);
    if(firstnumber>secondnumber)
    {
        printf("\nThe first number, %d, is greater\n", firstnumber);
    }
    else if(secondnumber>firstnumber)
    {
       printf("\nThe second number, %d, is greater.\n", secondnumber);
    }
    else
    {
        printf("\nThe first number, %d, is equal to the second number, %d.\n", firstnumber, secondnumber);
    }
}
