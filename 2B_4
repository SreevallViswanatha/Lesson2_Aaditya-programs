#include <stdio.h>
#include <stdlib.h>
//If the ages of Ram, Shyam and Ajay are input through the keyboard, WAP to determine the youngest of the three. Display the name and the age.
int main()
{

    int Ram, Shyam, Ajay;
    printf("Enter the age of Ram: ");
    scanf("%d", &Ram);
    printf("Enter the age of Ajay: ");
    scanf("%d", &Ajay);
    printf("Enter the age of Shyam: ");
    scanf("%d", &Shyam);

    if(Ram<Ajay)
    {
        if(Ram<Shyam)
        {
            printf("Ram, aged %d, is the youngest.", Ram);
        }
        else
        {
            printf("Shyam, aged %d, is the youngest.", Shyam);
        }
    }
    else
    {
        if(Ajay<Shyam)
        {
            printf("Ajay, aged %d, is the youngest.", Ajay);
        }
        else
        {
            printf("Shyam, aged %d, is the youngest.", Shyam);
        }
    }
}
