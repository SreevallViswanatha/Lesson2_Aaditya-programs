#include <stdio.h>
#include <stdlib.h>
//Any year is an input through the keyboard. WAP to determine whether the year is a leap year or not.
int main()
{

   int year;
   printf("Enter a year: ");
   scanf("%d", &year);

   if(year%100==0)
   {
       if(year%400==0)
       {
           printf("The year is a leap year.");
       }
       else
       {
           printf("The year is not a leap year.");
       }

   }
    else if(year%4==0)
       {
           printf("The year is a leap year.");
       }
    else
       {
           printf("The year is not a leap year.");
       }
}
