#include <stdio.h>
#include <stdlib.h>
//A five-digit number is entered through the keyboard.
//Write a program to obtain the reversed number and to determine whether the original and reversed numbers are equal or not.
int main()
{

    int num, number, revnum, remainder;
    printf("\nEnter a five-digit number: ");
    scanf("%d", &number);
    num=number;
    remainder=number%10;
    revnum=10000*remainder;
    number=number/10;
    remainder=number%10;
    revnum=revnum+(1000*remainder);
    number=number/10;
    remainder=number%10;
    revnum=revnum+(100*remainder);
     number=number/10;
    remainder=number%10;
    revnum=revnum+(10*remainder);
     number=number/10;
    revnum=revnum+number;

    if(revnum==num)
    {
        printf("\nThe original number, %d, is equal to the reverse number, %d.\n", num, revnum);
    }
    else
    {
        printf("\nThe original number, %d, is different from the reverse number, %d.\n", num, revnum);
    }
}

