#include <stdio.h>
#include <stdlib.h>
//WAP to ask user to enter a number less than 10, if the user enters a number less than 10, print : 
//“What an obedient servant you are!” else print “the number is not less than 10 ”.
int main()
{

   int number;
   printf("\nEnter a number less than 10: ");
   scanf("%d", &number);
   if(number<10)
   {
      printf("\nWhat an obedient servant you are!\n");
   }
   else
   {
       printf("\nThe number is not less than 10.\n");
   }
}
