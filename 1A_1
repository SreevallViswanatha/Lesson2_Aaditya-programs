#include <stdio.h>
#include <stdlib.h>
\\WAP to ask user to enter a number less than 10, if the user enters a number less than 10, print :
\\“What an obedient servant you are!” else print “the number is not less than 10 ”.
int main()
{

    int number;
    printf("Enter a number:");
    scanf("%d", &number);
    if(number<10)
    {
        printf("What an obedient servant you are.");
    }
    else
    {
        printf("The number is less than ten.");
    }
}
